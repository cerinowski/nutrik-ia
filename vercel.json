{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/webhook/stripe",
      "destination": "/api/index.js"
    },
    {
      "source": "/create-checkout-session",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*\\.html)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.css)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.js)",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}